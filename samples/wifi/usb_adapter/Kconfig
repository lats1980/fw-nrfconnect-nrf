source "Kconfig.zephyr"

menu "Nordic WiFi USB Adapter"

config WIFI_BULK_EP_MPS
	int
	default 512 if USB_DC_HAS_HS_SUPPORT
	default 64
	help
	  WiFi adapter bulk endpoint size

config NRF700X_ON_QSPI
	bool "Flag for QSPI"
	select NRFX_QSPI
	help
	  Enable this flag to include customized logic.

endmenu
